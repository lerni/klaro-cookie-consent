{
	"name": "lerni/klaro-cookie-consent",
	"type": "silverstripe-vendormodule",
	"description": "Klaro Silverstripe Integration",
	"keywords": [
		"Silverstripe",
		"cookie",
		"consent",
		"Klaro",
		"gdpr",
		"dsgvo",
		"eprivacy",
		"consent mode v2"
	],
	"homepage": "https://github.com/lerni/klaro-cookie-consent",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Lukas Erni",
			"homepage": "https://kraftausdruck.ch",
			"email": "le@kraftausdruck.ch",
			"role": "Developer"
		}
	],
	"require": {
		"silverstripe/cms": "^5",
		"silverstripe/siteconfig": "^5",
		"symbiote/silverstripe-gridfieldextensions": "^4"
	},
	"minimum-stability": "dev",
	"extra": {
		"installer-name": "klaro-cookie-consent",
		"expose": [
			"client/node_modules/klaro/dist"
		]
	},
	"support": {
		"issues": "https://github.com/lerni/klaro-cookie-consent/issues"
	},
	"suggest": {
		"lerni/silverstripe-tracking": "Google Analytics, Tag Manager & Microsoft Clarity Tracking Module for Silverstripe"
	},
	"autoload": {
		"psr-4": {
			"Kraftausdruck\\": "src/"
		}
	}
}
